{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "../",
    "paths": {
      "@utils/*": ["utils/*"],
      "@utils": ["utils/src/index.ts"],
      "@libs/*": ["libs/*"],
      "@libs": ["libs/src/index.ts"],
    }
  }
}
